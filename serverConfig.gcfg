[TLS]
    Cert            = "certs/server.crt"
    Key             = "certs/server.key"
    CaCert          = "certs/ca.crt"

[RuleSystem]
    Enabled         = true
    ModulePath      = "Module"
    Rulefile        = "ruleFile.rule"
    Worker          = 1
    RpcInterface    = "127.0.0.1:8000"

[LogServer]
    Enabled         = true
    RpcInterface    = "127.0.0.1:8000"